  "keywords": [
    "koishi",
    "bot",
    "required:database"
  ],
  "bugs": {
    "url": "https://github.com/yashinatsu/koishi-plugin-favor/issues"
  },
  "homepage": "https://github.com/yashinatsu/koishi-plugin-favor",
  "jest": {
    "moduleFileExtensions": [
